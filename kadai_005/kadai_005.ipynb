        "# 台形の上辺、下辺、高さを定義\n",
        "上辺 = 10\n",
        "下辺 = 20\n",
        "高さ = 5\n",
        "\n",
        "# 台形の面積を計算\n",
        "area = (上辺 + 下辺) * 高さ / 2\n",
        "\n",
        "# 面積を表示\n",
        "print(f\"台形の面積は {area} \")"
